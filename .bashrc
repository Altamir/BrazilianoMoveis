# .bashrc

# Source global definitions
if [ -f /etc/bashrc ]; then
        . /etc/bashrc
fi

# Uncomment the following line if you don't like systemctl's auto-paging feature:
# export SYSTEMD_PAGER=

# User specific aliases and functions
alias cp='/usr/local/bin/cp'
alias echo='/usr/local/bin/echo'
alias git='/usr/local/bin/git'
alias grep='/usr/local/bin/grep'
alias ll='/usr/local/bin/ll'
alias ls='/usr/local/bin/ls'
alias mkdir='/usr/local/bin/mkdir'
alias mv='/usr/local/bin/mv'
alias rm='/usr/local/bin/rm'
alias tar='/usr/local/bin/tar'
alias ulimit='/usr/local/bin/ulimit'
alias umbler='/usr/local/bin/umbler'
alias unzip='/usr/local/bin/unzip'
alias vi='/usr/local/bin/vim'
alias vim='/usr/local/bin/vim'
alias zip='/usr/local/bin/zip'
